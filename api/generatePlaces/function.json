{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },    
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "extensions": {
    "http": {
      "routePrefix": "",
      "cors": {
        "allowedOrigins": [
          "api.pawan.krd",
          "maps.googleapis.com"
        ],
        "allowedMethods": [
          "GET",
          "POST"
        ],
        "allowedHeaders": [
          "Content-Type",
          "Authorization"
        ]
      }
    }
  }
}
